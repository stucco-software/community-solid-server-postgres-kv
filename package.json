{
  "name": "css-postgres-kv",
  "version": "1.0.0-alpha.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stucco-software/community-solid-server-postgres-kv.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/css-postgres-kv",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/css-postgres-kv/^1.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/css-postgres-kv/^1.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/css-postgres-kv/^1.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/css-postgres-kv/^1.0.0/dist/": "dist/"
  },
  "files": [
    "config",
    "dist"
  ],
  "engines": {
    "node": ">=18.14"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:components",
    "build:components": "componentsjs-generator -s src -c dist/components -i .componentsignore -r cpk",
    "build:ts": "tsc",
    "lint": "npm run lint:eslint && npm run lint:markdown",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:markdown:fix",
    "lint:eslint": "eslint . --cache --max-warnings 0",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:markdown": "markdownlint-cli2",
    "lint:markdown:fix": "npm run lint:markdown -- --fix",
    "release": "commit-and-tag-version",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@solid/community-server": "^8.0.0-alpha.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^30.0.0",
    "commit-and-tag-version": "^12.4.2",
    "componentsjs-generator": "^4.0.1",
    "husky": "^4.3.8",
    "jest": "^30.0.0",
    "jest-util": "^30.2.0",
    "markdownlint-cli2": "^0.13.0",
    "opinionated-eslint-config": "^0.1.0",
    "postgres": "^3.4.8",
    "simple-git": "^3.25.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "commit-and-tag-version": {
    "scripts": {
      "postbump": "ts-node ./scripts/upgradeConfig.ts",
      "postchangelog": "markdownlint-cli2 --fix"
    },
    "writerOpts": {
      "commitsSort": false
    }
  }
}
